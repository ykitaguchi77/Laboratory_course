      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.datasets import fetch_california_housing\n",
        "\n",
        "# データセットの読み込み\n",
        "california = fetch_california_housing()\n",
        "\n",
        "# 説明変数\n",
        "X_array = california.data\n",
        "# 目的変数\n",
        "y_array = california.target\n",
        "\n",
        "# DataFrameの作成\n",
